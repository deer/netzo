{
  "uid": "resource-http-google-appsheet",
  "type": "resource",
  "status": "published",
  "name": "AppSheet",
  "description": "AppSheet is a low-code platform for building and deploying quick apps that connects to multiple data sources.",
  "labels": [
    "productivity",
    "low-code"
  ],
  "display": {
    "avatar": "https://raw.githubusercontent.com/netzo/netzo/main/packages/resources/resource-http-google-appsheet/icon.webp"
  },
  "links": [
    {
      "rel": "repository",
      "href": "https://github.com/netzo/netzo/tree/main/packages/resources/resource-http-google-appsheet"
    },
    {
      "rel": "readme",
      "href": "https://raw.githubusercontent.com/netzo/netzo/main/packages/resources/resource-http-google-appsheet/src/readme.md"
    }
  ],
  "item": {
    "_type": "resource",
    "type": "http",
    "base": {
      "baseURL": "https://api.appsheet.com/api/v2/apps/{{appId}}/tables",
      "headers": {
        "Content-Type": "application/json"
      },
      "authorization": {
        "type": "apiKey",
        "in": "header",
        "name": "ApplicationAccessKey",
        "value": "{{applicationAccessKey}}"
      }
    }
  }
}
