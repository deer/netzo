{
  "uid": "resource-http-github",
  "type": "resource",
  "status": "published",
  "name": "GitHub",
  "description": "Resource for the GitHub API",
  "labels": [
    "infrastructure"
  ],
  "display": {
    "imageUrl": "https://raw.githubusercontent.com/netzo/netzo/main/packages/resources/resource-http-github/icon.webp"
  },
  "links": [
    {
      "rel": "repository",
      "href": "https://github.com/netzo/netzo/tree/main/packages/resources/resource-http-github"
    },
    {
      "rel": "readme",
      "href": "https://raw.githubusercontent.com/netzo/netzo/main/packages/resources/resource-http-github/src/readme.md"
    }
  ],
  "schema": [
    {
      "component": "v-text-field",
      "name": "personalAccessToken",
      "label": "Personal Access Token"
    }
  ],
  "item": {
    "_type": "resource",
    "type": "http",
    "base": {
      "baseURL": "https://api.github.com",
      "headers": {
        "Content-Type": "application/json"
      },
      "authorization": {
        "type": "bearer",
        "token": "{{personalAccessToken}}"
      }
    }
  }
}