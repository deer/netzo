{
  "uid": "resource-http-pandadoc",
  "type": "resource",
  "version": "1.0.0",
  "status": "published",
  "name": "PandaDoc",
  "description": "Resource for the PandaDoc API",
  "labels": [
    "productivity"
  ],
  "display": {
    "imageUrl": "https://raw.githubusercontent.com/netzo/netzo/main/packages/resources/resource-http-pandadoc/icon.webp"
  },
  "links": [
    {
      "rel": "author",
      "href": "https://netzo.io",
      "title": "Netzo"
    },
    {
      "rel": "repository",
      "href": "https://github.com/netzo/netzo/tree/main/packages/resources/resource-http-pandadoc"
    },
    {
      "rel": "readme",
      "href": "https://raw.githubusercontent.com/netzo/netzo/main/packages/resources/resource-http-pandadoc/src/readme.md"
    }
  ],
  "schema": [
    {
      "component": "v-text-field",
      "name": "apiKeyWithPrefix",
      "label": "API Key (with prefix)",
      "default": "API-Key {{apiKey}}",
      "hint": "Replace {{apiKey}} with corresponding value."
    }
  ],
  "item": {
    "_type": "resource",
    "type": "http",
    "base": {
      "baseURL": "https://api.pandadoc.com/public/v1",
      "headers": {
        "Content-Type": "application/json"
      },
      "authorization": {
        "type": "apiKey",
        "in": "header",
        "name": "Authorization",
        "value": "{{apiKeyWithPrefix}}"
      }
    }
  }
}