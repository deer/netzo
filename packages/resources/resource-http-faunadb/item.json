{
  "uid": "resource-http-faunadb",
  "type": "resource",
  "version": "1.0.0",
  "status": "published",
  "name": "FaunaDB",
  "description": "Resource for the FaunaDB GraphQL API",
  "labels": [
    "database"
  ],
  "display": {
    "imageUrl": "https://raw.githubusercontent.com/netzo/netzo/main/packages/resources/resource-http-faunadb/icon.webp"
  },
  "links": [
    {
      "rel": "repository",
      "href": "https://github.com/netzo/netzo/tree/main/packages/resources/resource-http-faunadb"
    },
    {
      "rel": "readme",
      "href": "https://raw.githubusercontent.com/netzo/netzo/main/packages/resources/resource-http-faunadb/src/readme.md"
    }
  ],
  "schema": [
    {
      "component": "v-text-field",
      "name": "baseURL",
      "label": "Base URL",
      "hint": "Replace {{regionGroupHostname}} with corresponding value",
      "default": "https://{{regionGroupHostname}}/graphql"
    },
    {
      "component": "v-text-field",
      "name": "username",
      "label": "Username"
    },
    {
      "component": "v-text-field",
      "name": "password",
      "label": "Password"
    }
  ],
  "item": {
    "_type": "resource",
    "type": "http",
    "base": {
      "baseURL": "{{baseURL}}",
