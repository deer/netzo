{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "type": "object",
  "properties": {
    "Vehicle_Year": {
      "type": "string",
      "title": "Vehicle Year"
    },
    "Vehicle_Make": {
      "type": "string",
      "title": "Vehicle Make"
    },
    "Vehicle_Model": {
      "type": "string",
      "title": "Vehicle Model"
    },
    "Vehicle_Description": {
      "type": "string",
      "title": "Vehicle Description"
    },
    "First_image": {
      "type": "string",
      "title": "First Image"
    },
    "Vehicle_Engine_Type": {
      "type": "string",
      "title": "Vehicle Engine Type"
    },
    "Vehicle_kms": {
      "type": "number",
      "title": "Vehicle Kilometers",
      "minimum": 0
    },
    "Second_image": {
      "type": "string",
      "title": "Second Image"
    },
    "Third_image": {
      "type": "string",
      "title": "Third Image"
    },
    "Vehicle_Price": {
      "type": "number",
      "title": "Vehicle Price",
      "minimum": 0
    },
    "Vehicle_Price_Disclaimer": {
      "type": "string",
      "title": "Vehicle Price Disclaimer"
    },
    "Vehicle_Price_Method": {
      "type": "string",
      "title": "Vehicle Price Method"
    }
  }
}