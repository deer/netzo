{
  "lock": false,
  "tasks": {
    "start": "deno run -A --watch main.ts"
  },
  "fmt": {
    "semiColons": false,
    "singleQuote": true
  },
  "compilerOptions": {
    "jsx": "react-jsx",
    "jsxFactory": "h",
    "jsxFragmentFactory": "Fragment",
    "jsxImportSource": "preact"
  },
  "imports": {
    "@/": "./",
    "netzo/": "https://deno.land/x/netzo@v0.2.8/"
  }
}